<section class='session-new'>

  <header class="page-header">
    <h1>Log In</h1>
  </header>

  <%= @user.inspect %>

  <div class="row">
    <div class="col-sm-6">
      <%= form_for :user, url: '/login' do |form| %>
        <div class="form-group">
          <%= form.label :email %>
          <%= form.text_field :email, class: 'form-control', id: 'email' %>
        </div>
        <div class="form-group">
          <%= form.label :password %>
          <%= form.password_field :password, class: 'form-control', id: 'password' %>
        </div>
        <%= form.submit "Submit", class: 'btn btn-primary', id: 'submit' %>
      <% end %>
    </div>
  </div>

</section>